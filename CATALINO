#include <iostream>
using namespace std;

int main() {
    cout << "======================" << endl;
    cout << "||   MyCppApp Logo  ||" << endl;
    cout << "||      ^_^         ||" << endl;
    cout << "||    /     \\       ||" << endl;
    cout << "||   |  o o  |      ||" << endl;
    cout << "||    \\__^__/       ||" << endl;
    cout << "======================" << endl;

    return 0;
}
